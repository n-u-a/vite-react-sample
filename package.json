    "autoprefixer": "^10.4.20",
    "axios": "^1.9.0",
    "concurrently": "^9.1.0",
    "crypto-js": "^4.2.0",
    "history": "^5.3.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
