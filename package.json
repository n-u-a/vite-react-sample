{
  "name": "vite-react-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:tailwind": "npx tailwindcss -i ./src/index.css -o ./src/output.css --watch",
    "dev": "concurrently \"npm run dev:tailwind\" \"vite\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook:coverage": "test-storybook",
    "storybook:test": "start-server-and-test storybook http://127.0.0.1:6006 test-storybook:coverage",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@reduxjs/toolkit": "^2.3.0",
    "@types/lodash": "^4.17.13",
